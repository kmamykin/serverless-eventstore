{
  "name": "http",
  "runtime": "nodejs",
  "description": "Lambda function for HTTP handler",
  "customName": false,
  "customRole": false,
  "handler": "functions/http/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"]
    }
  },
  "endpoints": [
    {
      "path": "{0}",
      "method": "GET",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}",
      "method": "POST",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}",
      "method": "PUT",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}",
      "method": "DELETE",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}/{1}",
      "method": "GET",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}/{1}",
      "method": "POST",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}/{1}",
      "method": "PUT",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    },
    {
      "path": "{0}/{1}",
      "method": "DELETE",
      "type": "AWS",
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplates}",
      "responses": "$${apiResponses}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
